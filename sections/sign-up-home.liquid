<section class="signup-banner signup-banner-home">
  <div class="signup-banner-img">
   <img src="{{section.settings.image |img_url:'master'}}" alt=""/> 
  </div>
  <div class="signup-banner-cont">
    <div class="container">
      <div class="row">
        <div class="col-md-6">
          <div class="home_signup_img">
        <img src="{{section.settings.red_image |img_url:'master'}}" alt=""/>
          </div>
        </div>
        <div class="col-md-6">
          <div class="signup-banner-content-form">
            
            <div class="banner-form-part" id="sign_up_form">
            <h2>{{section.settings.heading}}</h2>
            <p>{{section.settings.content}}</p>
            <div class="klaviyo-form-VZNcDw"></div>
          </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
