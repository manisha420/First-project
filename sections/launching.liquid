<section class="launching-part-main">
  <div class="container">
    <div class="row">
      <div class="col-md-6">
        <div class="launching-part-content">
          <h2>{{section.settings.heading}}</h2>
          <h3>{{section.settings.subheading}}</h3>
          <h4>{{section.settings.subheading_two}}</h4>
          <p>{{section.settings.content}}</p>
          <p>{{section.settings.copyright}}</p>
        </div>
      </div>
      <div class="col-md-6">
         <div class="launching-image-part">
           {% for block in section.blocks %}  
           <div class="launching-image-div">
        	<div class="row">
              <div class="col-6">
              <img src="{{block.settings.launch_image | img_url:'master'}}" alt=""/>
              </div>
              <div class="col-6">
              <img src="{{block.settings.launch_image_two | img_url:'master'}}" alt=""/>
              </div>
           </div>
           </div>
           {% endfor %}
          
           <div class="launching-red-letter text-center">
             <img src="{{section.settings.center_image | img_url:'master'}}" alt=""/>
           </div>
        </div>
      </div>
    </div>
  </div>
  <div class="grey-tri">
     <img src="{{section.settings.arrow_image |img_url:'master'}}" alt=""/>
  </div>
</section>

{% schema %}
  {
    "name": "Launching",
    "settings": [
  { 
      "type": "image_picker",
      "id": "center_image",
      "label": "Center Image" 
  }, 
{ 
      "type": "text",
      "id": "heading",
      "label": "heading",
      "default": "Sub-heading"
},
{ 
      "type": "text",
      "id": "subheading",
      "label": "Sub-heading",
      "default": "Sub-heading"
},
{ 
      "type": "text",
      "id": "subheading_two",
      "label": "Sub-heading",
      "default": "Sub-heading"
},
{ 
      "type": "text",
      "id": "content",
      "label": "Content"
},
{ 
      "type": "text",
      "id": "copyright",
      "label": "Text"
},
{ 
      "type": "image_picker",
      "id": "arrow_image",
      "label": "Image" 
  }
],
 "blocks": [
    {
      "type": "image",
      "name": "image",
      "settings": [
    { 
      "type": "image_picker",
      "id": "launch_image",
      "label": "Launch Image" 
  }, 
  { 
      "type": "image_picker",
      "id": "launch_image_two",
      "label": "Launch Image" 
  }
 ]
}
],
"presets": [
    {
      "category": "Launching",
      "name": "Launching",
      "settings": {}
    }
  ]
  }
{% endschema %}

